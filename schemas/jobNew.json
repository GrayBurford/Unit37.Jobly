{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "testTitle"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "minimum" : 0,
            "title": "The salary Schema",
            "examples": [
                100000
            ]
        },
        "equity": {
            "type": "string",
            "minimum" : 0,
            "maximum" : 1.0,
            "default" : 0.0,
            "title": "The equity Schema",
            "examples": [
                "0.4"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "testCompany"
            ]
        }
    },
    "examples": [{
        "id": 1,
        "title": "testTitle",
        "salary": 100000,
        "equity": "0.4",
        "companyHandle": "testCompany"
    }]
}